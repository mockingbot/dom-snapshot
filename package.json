{
  "name": "dom-snapshot",
  "version": "0.1.5",
  "author": "mockingbot",
  "license": "MIT",
  "description": "Get `<canvas>` from DOM string through SVG `<foreignObject>`.",
  "keywords": [
    "snapshot",
    "DOM",
    "canvas",
    "SVG",
    "foreignObject"
  ],
  "homepage": "https://github.com/mockingbot/dom-snapshot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockingbot/dom-snapshot.git"
  },
  "bugs": {
    "url": "https://github.com/mockingbot/dom-snapshot/issues"
  },
  "main": "library/index.js",
  "scripts": {
    "build": "npm run build-clear && npm run build-library && npm run build-module",
    "build-clear": "dr-js -m rm -a ./library ./module",
    "build-library": "cross-env NODE_ENV=production BABEL_ENV=library babel ./source --out-dir ./library --ignore test.js",
    "build-library-dev": "cross-env NODE_ENV=development BABEL_ENV=library-dev babel ./source --out-dir ./library --ignore test.js --watch",
    "build-module": "cross-env NODE_ENV=production BABEL_ENV=module babel ./source --out-dir ./module --ignore test.js",
    "build-example": "cross-env NODE_ENV=production webpack --config .webpackrc.js",
    "build-example-dev": "cross-env NODE_ENV=development webpack --config .webpackrc.js --watch",
    "prepack": "npm run build",
    "publish-latest": "npm publish --userconfig ~/thatbean.npmrc",
    "publish-dev": "npm publish --tag dev --userconfig ~/thatbean.npmrc"
  },
  "devDependencies": {
    "dr-js": "^0.9.1",
    "dev-dep-web": "^0.0.11"
  }
}
