{
  "private": true,
  "name": "dom-snapshot",
  "version": "0.2.1-dev.2",
  "author": "mockingbot",
  "license": "MIT",
  "description": "Get `<canvas>` from DOM string through SVG `<foreignObject>`.",
  "keywords": [
    "snapshot",
    "DOM",
    "canvas",
    "SVG",
    "foreignObject"
  ],
  "repository": "github:mockingbot/dom-snapshot",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "babel-node ./script/generateSpec",
    "// build-dev ===================": "",
    "build-library-dev": "cross-env BABEL_ENV=dev npm run build-library -- --watch",
    "build-example-dev": "babel-node ./script/webpack development watch",
    "// build =======================": "",
    "build-library": "babel ./source --out-dir ./output-gitignore/library",
    "build-module": "cross-env BABEL_ENV=module babel ./source --out-dir ./output-gitignore/module",
    "build-example": "babel-node ./script/webpack production profile",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "devDependencies": {
    "dr-dev": "0.0.2-dev.0",
    "dr-dev-web": "0.0.2-dev.0"
  }
}
