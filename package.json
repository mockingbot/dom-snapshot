{
  "private": true,
  "name": "dom-snapshot",
  "version": "0.2.2-dev.1",
  "description": "Get `<canvas>` from DOM string through SVG `<foreignObject>`.",
  "author": "mockingbot",
  "license": "MIT",
  "keywords": [
    "snapshot",
    "DOM",
    "canvas",
    "SVG",
    "foreignObject"
  ],
  "repository": "github:mockingbot/dom-snapshot",
  "scripts": {
    "// script ======================": "",
    "script-pack": "node -r @babel/register ./script pack",
    "script-publish": "node -r @babel/register ./script pack publish",
    "script-publish-dev": "node -r @babel/register ./script pack publish-dev",
    "script-generate-spec": "node -r @babel/register ./script/generateSpec",
    "// build =======================": "",
    "build-library": "cross-env BABEL_ENV=all-transform babel ./source --out-dir ./output-gitignore/library",
    "build-library-dev": "cross-env BABEL_ENV=dev npm run build-library -- --watch",
    "build-module": "cross-env BABEL_ENV=module babel ./source --out-dir ./output-gitignore/module",
    "build-example": "node -r @babel/register ./script/webpack production profile",
    "build-example-dev": "node -r @babel/register ./script/webpack development watch",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json example/",
    "lint": "eslint ./source/ ./script/",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "devDependencies": {
    "dr-dev": "0.0.7-dev.1",
    "dr-dev-web": "0.0.7-dev.1"
  }
}
